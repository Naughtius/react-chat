   },
   "homepage": "https://github.com/Naughtius/react-chat#readme",
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "concurrently": "^5.3.0",
      "cross-env": "^7.0.2",
      "express": "^4.17.1",
      "express-validator": "^6.6.1",
      "jsonwebtoken": "^8.5.1",
      "mongoose": "^5.10.5",
      "nodemon": "^2.0.4",
      "notistack": "^1.0.0"
   }
